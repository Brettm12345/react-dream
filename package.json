{
  "name": "react-dream",
  "version": "0.0.1",
  "description": "Fantasy Land type for React Components",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "sagui build",
    "dist": "babel src/ -d dist/ --ignore spec.js,example/",
    "format": "sagui format",
    "prepare": "npm run test && npm run dist",
    "start": "sagui develop --port 3000",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "sagui test:lint",
    "test:typecheck": "sagui test:typecheck",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xaviervia/react-dream.git"
  },
  "keywords": [
    "react",
    "fantasy-land",
    "fantasyland",
    "functional",
    "functional-programming",
    "fp"
  ],
  "author": "Xavier Via <fernando.via@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xaviervia/react-dream/issues"
  },
  "homepage": "https://github.com/xaviervia/react-dream#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "jest": "^20.0.4",
    "prettier": "^1.5.3",
    "ramda": "^0.24.1",
    "react": "^16.0.0-beta.3",
    "react-dom": "^16.0.0-beta.3",
    "react-test-renderer": "^16.0.0-beta.3",
    "recompose": "^0.24.0",
    "sagui": "^11.0.2"
  },
  "peerDependencies": {
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "recompose": "^0.24.0"
  }
}
